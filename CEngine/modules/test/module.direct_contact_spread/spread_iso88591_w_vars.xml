<?xml version="1.0" encoding="ISO-8859-1" ?>
<naadsm:disease-simulation
  xmlns:naadsm="http://www.naadsm.org/schema"
  xmlns:xdf="http://xml.gsfc.nasa.gov/XDF">
  <description>Specs for a sample simulation run.</description>
  <num-days>10</num-days>
  <num-runs>1</num-runs>

  <models>

    <disease-model production-type="ãêïòú">
      <latent-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </latent-period>
      <infectious-subclinical-period>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-subclinical-period>
      <infectious-clinical-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-clinical-period>
      <immunity-period>
        <point>3</point>
        <units><xdf:unit>day</xdf:unit></units>
      </immunity-period>
    </disease-model>

    <disease-model production-type="çÇñÑ">
      <latent-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </latent-period>
      <infectious-subclinical-period>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-subclinical-period>
      <infectious-clinical-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-clinical-period>
      <immunity-period>
        <point>3</point>
        <units><xdf:unit>day</xdf:unit></units>
      </immunity-period>
    </disease-model>

    <contact-spread-model from-production-type="ãêïòú" to-production-type="çÇñÑ" contact-type="direct">
      <fixed-movement-rate>
        <value>1</value>
        <units><xdf:unit power="-1">day</xdf:unit></units>
      </fixed-movement-rate>
      <distance>
        <point>10</point>
        <units><xdf:unit>km</xdf:unit></units>
      </distance>
      <delay>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </delay>
      <prob-infect>1.0</prob-infect>
      <!-- Movement is unaffected by public knowledge of an outbreak. -->
      <movement-control>
        <value>0</value> <value>1.0</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unitless /></units>
      </movement-control>
      <latent-units-can-infect>false</latent-units-can-infect>
    </contact-spread-model>

    <infection-monitor>
      <ratio-period>
        <value>14</value>
        <units><xdf:unit>day</xdf:unit></units>
      </ratio-period>
      <output>
        <variable-name>infnU</variable-name>
        <broken-down>yes</broken-down>
        <frequency>daily</frequency>
      </output>
    </infection-monitor>

    <conflict-resolver></conflict-resolver>

  </models>

</naadsm:disease-simulation>
