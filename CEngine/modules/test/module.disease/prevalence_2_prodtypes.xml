<?xml version="1.0" encoding="UTF-8"?>
<naadsm:disease-simulation
  xmlns:naadsm="http://www.naadsm.org/schema"
  xmlns:xdf="http://xml.gsfc.nasa.gov/XDF">
  <description>Specs for a sample simulation run.</description>
  <num-days>4</num-days>
  <num-runs>1</num-runs>

  <models>

    <disease-model production-type="Cattle">
      <latent-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </latent-period>
      <infectious-subclinical-period>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-subclinical-period>
      <infectious-clinical-period>
        <point>2</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-clinical-period>
      <immunity-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </immunity-period>
      <prevalence>
        <value>0</value> <value>0</value>
        <value>1</value> <value>0.35</value>
        <value>2</value> <value>0.7</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unitless /></units>
      </prevalence>
    </disease-model>

    <disease-model production-type="Pigs">
      <latent-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </latent-period>
      <infectious-subclinical-period>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-subclinical-period>
      <infectious-clinical-period>
        <point>2</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-clinical-period>
      <immunity-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </immunity-period>
      <prevalence>
        <value>0</value> <value>0</value>
        <value>1</value> <value>0.35</value>
        <value>2</value> <value>0.7</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unitless /></units>
      </prevalence>
    </disease-model>

    <contact-spread-model from-production-type="Cattle" to-production-type="Cattle" contact-type="direct">
      <fixed-movement-rate>
        <value>1</value>
        <units><xdf:unit power="-1">day</xdf:unit></units>
      </fixed-movement-rate>
      <distance>
        <point>1</point>
        <units><xdf:unit>km</xdf:unit></units>
      </distance>
      <delay>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </delay>
      <!-- No movement control. -->
      <movement-control>
        <value>0</value> <value>1.0</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unitless /></units>
      </movement-control>
      <latent-units-can-infect>false</latent-units-can-infect>
      <subclinical-units-can-infect>true</subclinical-units-can-infect>
    </contact-spread-model>

    <contact-spread-model from-production-type="Pigs" to-production-type="Pigs" contact-type="direct">
      <fixed-movement-rate>
        <value>1</value>
        <units><xdf:unit power="-1">day</xdf:unit></units>
      </fixed-movement-rate>
      <distance>
        <point>1</point>
        <units><xdf:unit>km</xdf:unit></units>
      </distance>
      <delay>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </delay>
      <!-- No movement control. -->
      <movement-control>
        <value>0</value> <value>1.0</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unitless /></units>
      </movement-control>
      <latent-units-can-infect>false</latent-units-can-infect>
      <subclinical-units-can-infect>true</subclinical-units-can-infect>
    </contact-spread-model>

    <conflict-resolver />

    <state-table-writer />

  </models>

</naadsm:disease-simulation>
